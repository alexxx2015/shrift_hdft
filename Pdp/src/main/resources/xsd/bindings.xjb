    <jxb:bindings node="//xs:complexType[@name='RepLimType']">
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.condition.operators.RepLim" />
      <inheritance:extends>de.tum.in.i22.cm.pdp.internal.condition.Operator</inheritance:extends> 
    </jxb:bindings>

    <jxb:bindings node="//xs:complexType[@name='RepSinceType']">
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.condition.operators.RepSince" />
      <inheritance:extends>de.tum.in.i22.cm.pdp.internal.condition.Operator</inheritance:extends> 
    </jxb:bindings>

    <jxb:bindings node="//xs:complexType[@name='RepMaxType']">
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.condition.operators.RepMax" />
      <inheritance:extends>de.tum.in.i22.cm.pdp.internal.condition.Operator</inheritance:extends> 
    </jxb:bindings>
    
    <jxb:bindings node="//xs:complexType[@name='StateBasedOperatorType']">
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.condition.operators.StateBasedOperator" />
      <inheritance:extends>de.tum.in.i22.cm.pdp.internal.condition.Operator</inheritance:extends> 
    </jxb:bindings>
    
    <jxb:bindings node="//xs:complexType[@name='EvalOperatorType']">
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.condition.operators.EvalOperator" />
      <inheritance:extends>de.tum.in.i22.cm.pdp.internal.condition.Operator</inheritance:extends> 
    </jxb:bindings>    

    <jxb:bindings node="//xs:complexType[@name='EventMatchingOperatorType']">
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.condition.operators.EventMatchOperator" />
      <inheritance:extends>de.tum.in.i22.cm.pdp.internal.condition.Operator</inheritance:extends> 
      
      <jxb:bindings node="//xs:element[@name='paramMatch']">
        <jxb:property name="params" />
      </jxb:bindings>
    </jxb:bindings>

    <jxb:bindings node="//xs:complexType[@name='ParamMatchType']">
      <!--<jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.Param" />-->
      <jxb:class implClass = "de.tum.in.i22.cm.pdp.internal.ParamMatch" />
    </jxb:bindings>    
  </jxb:bindings>


  <jxb:bindings schemaLocation="event.xsd">
    <jxb:schemaBindings>
      <jxb:package name="de.tum.in.i22.cm.pdp.xsd.event" />
    </jxb:schemaBindings>  
  </jxb:bindings>
  
  <jxb:bindings schemaLocation="action.xsd">
    <jxb:schemaBindings>
      <jxb:package name="de.tum.in.i22.cm.pdp.xsd.action" />
    </jxb:schemaBindings>  
  </jxb:bindings>    

  <jxb:bindings schemaLocation="time.xsd">
    <jxb:schemaBindings>
      <jxb:package name="de.tum.in.i22.cm.pdp.xsd.time" />
    </jxb:schemaBindings>  
  </jxb:bindings>    
  
